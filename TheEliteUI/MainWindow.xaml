<Window x:Class="TheEliteUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" Background="Gainsboro" WindowStartupLocation="CenterScreen"
        Title="MainWindow" Height="900" Width="1600">
    <DockPanel>
        <StackPanel Orientation="Horizontal" DockPanel.Dock="Top">
            <DatePicker Name="RankingDatePicker" />
            <Button Margin="5, 0, 0, 0" Name="AnimationButton" Click="AnimationButton_Click" />
        </StackPanel>
        <Canvas DockPanel.Dock="Bottom" Margin="0, 5, 0, 0" Name="RankingView" />
    </DockPanel>
</Window>
